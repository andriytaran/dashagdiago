</div>

<script>
  document.getElementById('athleticform').addEventListener('submit', function () {
    submit();
  });


  function submit() {
    let res = [];
    let carriesVal = document.getElementById("carries").value;
    let completionsVal = document.getElementById("completions").value;
    let interceptionsThrownVal = document.getElementById("interceptionsThrown").value;
    let passingYardsVal = document.getElementById("passingYards").value;
    let recievingYardsVal = document.getElementById("recievingYards").value;
    let receptionsVal = document.getElementById("receptions").value;
    let rushingTouchdownsVal = document.getElementById("rushingTouchdowns").value;
    let rushingYardsVal = document.getElementById("rushingYards").value;
    let sacksVal = document.getElementById("sacks").value;
    let soloTackleVal = document.getElementById("soloTackle").value;
    let tacklesForLossVal = document.getElementById("tacklesForLoss").value;
    let totalTacklesVal = document.getElementById("totalTackles").value;
    let touchdownsThrownVal = document.getElementById("touchdownsThrown").value;
    let fortyVal = document.getElementById("forty").value;
    let gamesPlayedVal = document.getElementById("gamesPlayed").value;
    let gamesStartedVal = document.getElementById("gamesStarted").value;
    let heightVal = document.getElementById("height").value;
    let verticalVal = document.getElementById("vertical").value;
    let weightVal = document.getElementById("weight").value;
    res.push(completionsVal);
    res.push(interceptionsThrownVal);
    res.push(passingYardsVal);
    res.push(recievingYardsVal);
    res.push(receptionsVal);
    res.push(rushingTouchdownsVal);
    res.push(rushingYardsVal);
    res.push(sacksVal);
    res.push(soloTackleVal);
    res.push(tacklesForLossVal);
    res.push(totalTacklesVal);
    res.push(touchdownsThrownVal);
    res.push(fortyVal);
    res.push(gamesPlayedVal);
    res.push(gamesStartedVal);
    res.push(heightVal);
    res.push(verticalVal);
    res.push(heightVal);
    alert(res);
  }
</script>
