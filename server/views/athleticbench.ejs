              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Touchfowns Thrown" id="touchdownsThrown" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="forty">Forty Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Forty" id="forty" />
