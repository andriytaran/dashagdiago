<aside class="sidebar-wrap" id="sidebar">
  <div class="sidebar">
    <div class="sidebar__toggler" id="sidebarToggler">
      <div class="sidebar__toggler-line"></div>
      <div class="sidebar__toggler-arrow">
        <img src="../updatedDesign/img/toggle-btn.svg" alt="">
      </div>
    </div>
    <div class="sidebar__heading">
      View By
    </div>
    <div class="sidebar__nav accordion" id="sidebar-nav">
      <div class="sidebar__nav-cat">
        <div class="sidebar__cat-heading" id="offsense">
          <h2 class="mb-0">
            <button class="sidebar__btn collapsed" type="button" data-toggle="collapse"
                    data-target="#offsenseCollapse" aria-expanded="false" aria-controls="offsenseCollapse">
              Offense
            </button>
          </h2>
        </div>
        <div id="offsenseCollapse" class="collapse" aria-labelledby="offsense" data-parent="#sidebar-nav">
          <div class="sidebar__cat-list" id="sidebarCategory">
            <div class="sidebar__dropdown dropdown">
              <button class="sidebar__cat-item" type="button" data-toggle="collapse"
                      data-target="#quarterback" aria-expanded="false" aria-controls="quarterback">
                Quarterback
              </button>
              <div class="sidebar__cat-item-dropdown collapse" data-parent="#sidebarCategory" id="quarterback"
                   aria-labelledby="quarterback">
                <div class="sidebar__cat-dropdown-list">
                  <a class="" href="/dashboard-position/?position=QBPRO">Pro Style</a>
                  <a class="" href="/dashboard-position/?position=QBDUAL">Dual</a>
                </div>
              </div>
            </div>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=RB&highschoolGraduationYear=<%= highschoolGraduationYear %>">Running Back</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=WR&highschoolGraduationYear=<%= highschoolGraduationYear %>">Wide Receiver</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=OG&highschoolGraduationYear=<%= highschoolGraduationYear %>">Offensive Guard</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=OT&highschoolGraduationYear=<%= highschoolGraduationYear %>">Offensive Tackle</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=OC&highschoolGraduationYear=<%= highschoolGraduationYear %>">Offensive Center</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=TE&highschoolGraduationYear=<%= highschoolGraduationYear %>">Tight End</a>
          </div>
        </div>
      </div>

      <div class="sidebar__nav-cat">
        <div class="sidebar__cat-heading" id="defense">
          <h2 class="mb-0">
            <button class="sidebar__btn collapsed" type="button" data-toggle="collapse"
                    data-target="#defenseCollapse" aria-expanded="false" aria-controls="defenseCollapse">
              Defense
            </button>
          </h2>
        </div>
        <div id="defenseCollapse" class="collapse" aria-labelledby="defense" data-parent="#sidebar-nav">
          <div class="sidebar__cat-list">
            <a class="sidebar__cat-item" href="/dashboard-position/?position=DL&highschoolGraduationYear=<%= highschoolGraduationYear %>">Defensive Line</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=DE&highschoolGraduationYear=<%= highschoolGraduationYear %>">Defensive End</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=DT&highschoolGraduationYear=<%= highschoolGraduationYear %>">Defensive Tackle</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=OLB&highschoolGraduationYear=<%= highschoolGraduationYear %>">Outside Linebacker</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=MLB&highschoolGraduationYear=<%= highschoolGraduationYear %>">Middle Linebacker</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=ILB&highschoolGraduationYear=<%= highschoolGraduationYear %>">Inside Linebacker</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=DB&highschoolGraduationYear=<%= highschoolGraduationYear %>">Defensive Back</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=S&highschoolGraduationYear=<%= highschoolGraduationYear %>">Safety</a>
          </div>
        </div>
      </div>
      <div class="sidebar__nav-cat">
        <div class="sidebar__cat-heading" id="specialTeams">
          <h2 class="mb-0">
            <button class="sidebar__btn collapsed" type="button" data-toggle="collapse"
                    data-target="#specialTeamsCollapse" aria-expanded="false" aria-controls="specialTeamsCollapse">
              Special Teams
            </button>
          </h2>
        </div>
        <div id="specialTeamsCollapse" class="collapse" aria-labelledby="specialTeams"
             data-parent="#sidebar-nav">
          <div class="sidebar__cat-list">
            <a class="sidebar__cat-item" href="/dashboard-position/?position=P&highschoolGraduationYear=<%= highschoolGraduationYear %>">Punter</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=K&highschoolGraduationYear=<%= highschoolGraduationYear %>">Kicker</a>
            <a class="sidebar__cat-item" href="/dashboard-position/?position=LS&highschoolGraduationYear=<%= highschoolGraduationYear %>">Long Snapper</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</aside>
