<script>

  updateHeaderLinks();

  document.getElementById('newathleteinfo_submit').addEventListener('click', function () {
    submit();
  });

  function submit() {

    const getString = elementId => {
      const elem = document.getElementById(elementId);
      if (elem) {
        return elem.value
      }
      return null
    };

    const getFloat = elementId => {
      const elem = document.getElementById(elementId);
      let result = 0;

      if (elem) {
        const float = parseFloat(elem.value);
        result = isNaN(float) ? 0 : float;
      }

      return result
    }

    let pos = getString('position');
    const position = getPositionValueOnSelect(pos)
    const highschoolGraduationYear = getFloat('hsgyear');

    const athleteInfo = {
      fname: getString('firstName'),
      lname: getString('lastName'),
      twitter: getFloat('twitter'),
      instagram: getFloat('instagram'),
      snapchat: getFloat('snapchat'),
      position,
      offenseDefense: getFloat('-'),
      facebook: getFloat('facebook'),
      // school: getString('school'),
      hometown: getString('hometown'),
      state: getString('state'),
      height: getFloat('height'),
      weight: getFloat('weight'),
      highschoolGraduationYear,
      collegeYear: getFloat('collegeYear'),
      momParent: getFloat('momParent'),
      dadParent: getFloat('dadParent'),
      grandmotherParent: getFloat('grandmotherParent'),
      grandfatherParent: getFloat('grandfatherParent'),
      guardian: getFloat('guardian'),
      multisportAthlete: getFloat('multistportAthlete'),
      forty: getFloat('forty'),
      vertical: getFloat('vertical'),
      gamesStarted: getFloat('gamesStarted'),
      gamesPlayed: getFloat('gamesPlayed'),
      completions: getFloat('completations'),
      attempts: getFloat('attempts'),
      passingYards: getFloat('paasingYards'),
      touchdownsThrown: getFloat('touchdownsThrown'),
      interceptionsThrown: getFloat('interceptionsThrown'),
      kickoffYards: getFloat('-'),
      averageKickoff: getFloat('-'),
      touchBacks: getFloat('-'),
      totalPuntYards: getFloat('-'),
      averagePunt: getFloat('-'),
      fieldGoalPercentage: getFloat('-'),
      carries: getFloat('carries'),
      rushingYards: getFloat('rushingYards'),
      rushingTouchdowns: getFloat('rushingTouchdowns'),
      receptions: getFloat('recieptions'),
      receivingYards: getFloat('-'),
      recievingTouchdowns: getFloat('-'),
      soloTackles: getFloat('soloTrackles'),
      totalTackles: getFloat('totalTrackles'),
      sacks: getFloat('sacks'),
      tacklesForLoss: getFloat('tracklessForLoss'),
      interceptions: getFloat('-'),
      gpa: getFloat('GPA'),
      sat: getFloat('SAT'),
      act: getFloat('ACT'),
      coreGpa: getFloat('coreGPA'),
      emotionalIntelBehavior: getFloat('behavior'),
      emotionalIntelReflection: getFloat('reflection'),
      emotionalIntelTeamWork: getFloat('teamwork'),
      emotionalIntelRelationships: getFloat('relationships'),
      emotionalIntelAccountability: getFloat('accountability'),
      emotionalIntelResponsibility: getFloat('responsibility'),
      emotionalIntelIndependence: getFloat('independence'),
      socialTwitterSentiment: getFloat('twitterSentiment'),
      twitterFollowers: getFloat('twitterFollowers'),
      socialInstagramSentiment: getFloat('instagramSentiment'),
      instagramFollowers: getFloat('instagramFollowers'),
      facebookSentiment: getFloat('facebookSentiment'),
      newsMediaCoverageSentiment: getFloat('newsMediaCoverageSentiment'),
      newsMediaCoverageMentions: getFloat('newsMediaCoverageMentions'),
      newsMediaCoverageNational: getFloat('newsMediaCoverageNational'),
      newsMediaCoverageRegional: getFloat('newsMediaCoverageRegional'),
      pressReleaseSentiment: getFloat('pressReleaseSentiment'),
      pressReleaseSentimentCounter: getFloat('pressReleaseSentimentCounter'),
      coreAttributesCompetitiveness: getFloat('competitiveness'),
      coreAttributesPersistence: getFloat('persistence'),
      coreAttributesWorkEthic: getFloat('workEthic'),
      coreAttributesTeamOrientation: getFloat('teamOrentation'),
      coreAttributesMastery: getFloat('mastery'),
      coreAttributesOverallScore: getFloat('coreAttributesOverallScore')
    }

    const id = getFloat('playerId');

    const redirect = () => {
      window.location = `/dashboard-position/?position=${position}&highschoolGraduationYear=${highschoolGraduationYear || 'ALL'}`;
    }

    if (id){
      athleteInfo.id = id;
      updatePlayer(athleteInfo, () => setTimeout(redirect, 1000))
    } else {
      createPlayer(athleteInfo, () => setTimeout(redirect, 1000))
    }

  }

</script>

