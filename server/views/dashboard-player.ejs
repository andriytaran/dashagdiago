<% layout('layout_main') -%>
<div id="content" class="content">
  <!-- begin breadcrumb -->
  <ol class="breadcrumb pull-right">
    <li class="breadcrumb-item"><a href="javascript:;">Home</a></li>
    <li class="breadcrumb-item active">Dashboard</li>
  </ol>
  <!-- end breadcrumb -->
  <!-- begin page-header -->
  <h1 class="page-header">Individual Player Dashboard</h1>
  <!-- end page-header -->


  <!-- begin row -->
  <div class="row">
    <!-- begin col-6 -->
    <div class="col-lg-12">
      <!-- begin panel -->
      <div class="panel panel-inverse" id="basicInfo" data-sortable-id="index-7">
        <div class="panel-heading">
          <h4 class="panel-title">Basic Player info</h4>
        </div>
        <div class="panel-body">
          <div id="basic-player-info"></div>
        </div>
      </div>
      <!-- end panel -->
    </div>
    <!-- end col-6 -->
  </div>
  <!-- end row -->

  <div class="row">
    <!-- begin row -->
    <!-- begin col-12 -->
    <div class="col-lg-12">
      <!-- begin panel -->
      <div class="panel panel-inverse" id="playerAttributesBlock" data-sortable-id="index-7">
        <div class="panel-heading">
          <div class="panel-heading-btn">
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i
                class="fa fa-expand"></i></a>
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i
                class="fa fa-redo"></i></a>
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i
                class="fa fa-minus"></i></a>
            <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
          </div>
          <h4 class="panel-title">Player Attributes</h4>
        </div>
        <div class="panel-body">
          <div id="player-attributes">
            <!-- start accordion -->
            <div class="accordion" id="accordionExample">
            </div>
            <!-- accordion end -->
          </div>
        </div>
      </div>
      <!-- end panel -->
    </div>
    <!-- end col-12 -->
  </div>
  <!-- end row -->
</div>

<!-- Modal -->
<div id="loginModal" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="container">

      <div class="row" id="pwd-container">
        <div class="col-md-4"></div>

        <div class="col-md-4">
          <section class="login-form">
            <form method="post" action="#" role="login">
              <img src="/assets/img/logo.png" class="img-responsive" alt="" style="width: 100px"/>
              <input type="email" name="email" placeholder="Email" required class="form-control input-lg" value="enteryourmail@gmail.com" />

              <input type="password" class="form-control input-lg" id="password" placeholder="Password" required="" />


              <div class="pwstrength_viewport_progress"></div>


              <button type="submit" name="go" class="btn btn-lg btn-primary btn-block">Sign in</button>
              <div>
                <a href="#">Create account</a> or <a href="#">reset password</a>
              </div>

            </form>

            <div class="form-links">
              <a href="#">www.website.com</a>
            </div>
          </section>
        </div>

        <div class="col-md-4"></div>


      </div>
    </div>

  </div>
</div>

<% stylesheet('/../assets/css/common.css') -%>
<% stylesheet('/../assets/css/barchart.css') -%>
<% stylesheet('/../assets/css/home.css') -%>
<% stylesheet('/../assets/css/login.css') -%>
<% stylesheet('/../assets/css/dashboard-player.css') -%>
<% script('/../assets/js/app/drawOverall.js') -%>
<% script('/../assets/js/app/drawbarchart.js') -%>
<% script('/../assets/js/app/drawplayerinfo.js') -%>
<% script('/../assets/js/app/home.js') -%>
<% script('/../assets/js/app/dashboard-player.js') -%>
