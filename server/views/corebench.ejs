<% layout('layout_main') -%>
<div id="content" class="content">
  <!-- begin breadcrumb -->
  <ol class="breadcrumb pull-right">
    <li class="breadcrumb-item"><a href="javascript:;">Home</a></li>
    <li class="breadcrumb-item active">Dashboard</li>
  </ol>
  <!-- end breadcrumb -->
  <!-- begin page-header -->
  <h1 class="page-header">
    <%=team %> Benchmark Input Page</h1>
  <!-- end page-header -->
  <!-- begin row -->
  <div class="row">
    <!-- begin col-6 -->
    <div class="col-lg-12">
      <!-- begin panel -->
      <div class="panel panel-inverse" id="basicInfo" data-sortable-id="index-7">
        <div class="panel-heading">
          <h4 class="panel-title">Core attributes benchmark input</h4>
        </div>
        <div class="panel-body">
          <form id="coreform">
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="pos">Position: </label>
              <div class="col-md-9">
                <select class="form-control" id="pos">
                  <% for(let i=0; i<positions.length; i++) { %>
                  <option>
                    <%= positions[i].title %>
                  </option>
                  <% } -%>
                </select>
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="competitiveness">Competitiveness Benchmark</label>
              <div class="col-md-5">
                <input class="form-control m-b-5" type="text" placeholder="Competitiveness" id="competitiveness" />
              </div>
              <div class="col-md-4">
                <input class="form-control m-b-5" type="text" placeholder="Competitiveness Factor" id="competitivenessF" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="mastery">Mastery Benchmark</label>
              <div class="col-md-5">
                <input class="form-control m-b-5" type="text" placeholder="Mastery" id="mastery" />
              </div>
              <div class="col-md-4">
                <input class="form-control m-b-5" type="text" placeholder="Mastery Factor" id="masteryF" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="persistence">Persistence Benchmark</label>
              <div class="col-md-5">
                <input class="form-control m-b-5" type="text" placeholder="Persistence" id="persistence" />
              </div>
              <div class="col-md-4">
                <input class="form-control m-b-5" type="text" placeholder="Persistence Factor" id="persistenceF" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="teamOrientation">Team Orientation Benchmark</label>
              <div class="col-md-5">
                <input class="form-control m-b-5" type="text" placeholder="Team Orientation" id="teamOrientation" />
              </div>
              <div class="col-md-4">
                <input class="form-control m-b-5" type="text" placeholder="Team Orientation Factor" id="teamOrientationF" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="workEthic">Work Ethic Benchmark</label>
              <div class="col-md-5">
                <input class="form-control m-b-5" type="text" placeholder="Work Ethic" id="workEthic" />
              </div>
              <div class="col-md-4">
                <input class="form-control m-b-5" type="text" placeholder="Work Ethic Factor" id="workEthicF" />
              </div>
            </div>
            <div class="form-group row m-b-15" style="display: flex;flex-direction: row;align-items: center;">
              <div class="subbtn col-md-5"><input type="submit" class="btn btn-lime" id="submit" value="Submit"></div>
              <div class="col-md-7" style="text-align: right"><a href="/addnewpillar">Add new Pillar</a></div>
            </div>
          </form>
        </div>
      </div>
      <!-- end panel -->
    </div>
    <!-- end col-6 -->
  </div>
  <!-- end row -->
</div>

<script>
  document.getElementById('coreform').addEventListener('submit', function () {
    submit();
  });


  // function updateAPIData (res, cb) {
  //   console.log(res);
  //   $.ajax({
  //     type: "POST",
  //     url: "/api/dashboard-data-update",
  //     data: JSON.stringify(props),
  //     contentType: "application/json; charset=utf-8",
  //     dataType: "json",
  //     success: cb
  //   });
  // }

  // function updateCoreAttributesData(body, cb) {
  //   updateAPIData({
  //     type: "pillar",
  //     pillar: "coreAttributes",
  //     position: position,
  //     body: body
  //   }, cb);
  // }

  function submit() {
    let pos = document.getElementById("pos").value;
    let position = [
      {title: 'Quarterback', value: 'QB'},
      {title: 'Running Back', value: 'RB'},
      {title: 'Wide Receiver', value: 'WR'},
      {title: 'Offensive Line', value: 'OL'},
      {title: 'Defensive Line', value: 'DL'},
      {title: 'Tight End', value: 'TE'},
      {title: 'Linebacker', value: 'LB'},
      {title: 'Corner Back', value: 'CB'},
      {title: 'Kicker', value: 'K'},
      {title: 'Defensive End', value: 'DE'},
      {title: 'Defensive Tackle', value: 'DT'},
      {title: 'Offensive Guard', value: 'OG'},
      {title: 'Offensive Tackler', value: 'OT'},
      {title: 'Defensive Back', value: 'DB'},
      {title: 'Punter', value: 'P'},
      {title: 'Safety', value: 'S'},
      {title: 'Long Snapper', value: 'LS'},
    ].reduce(function(acc, elem){
      if (elem.title === pos){
        acc = elem.value
      }
      return acc
    }, 'QB')

    let competitivenessVal = document.getElementById("competitiveness").value;
    let competitivenessFVal = document.getElementById("competitivenessF").value;
    let masteryVal = document.getElementById("mastery").value;
    let masteryFVal = document.getElementById("masteryF").value;
    let persistenceVal = document.getElementById("persistence").value;
    let persistenceFVal = document.getElementById("persistenceF").value;
    let teamOrientationVal = document.getElementById("teamOrientation").value;
    let teamOrientationFVal = document.getElementById("teamOrientationF").value;
    let workEthicVal = document.getElementById("workEthic").value;
    let workEthicFVal = document.getElementById("workEthicF").value;

    const res = {
      position: position,
      competitivenessVal: competitivenessVal,
      competitivenessFVal: competitivenessFVal,
      masteryVal: masteryVal,
      masteryFVal: masteryFVal,
      persistenceVal: persistenceVal,
      persistenceFVal: persistenceFVal,
      teamOrientationVal: teamOrientationVal,
      teamOrientationFVal: teamOrientationFVal,
      workEthicVal: workEthicVal,
      workEthicFVal: workEthicFVal
    };
    updateBenchmarks(position, {})
  }
</script>

