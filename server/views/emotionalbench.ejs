          </div> -->

        </div>
      </div>
      <!-- end panel -->
    </div>
    <!-- end col-6 -->
  </div>
  <!-- end row -->
</div>

<script>
  document.getElementById('emotionalform').addEventListener('submit', function () {
    submit();
  });


  function submit() {
    let res = [];
    let emotionalIntelAccountabilityVal = document.getElementById("emotionalIntelAccountability").value;
    let emotionalIntelBehaviorVal = document.getElementById("emotionalIntelBehavior").value;
    let emotionalIntelIndependenceVal = document.getElementById("emotionalIntelIndependence").value;
    let emotionalIntelReflectionVal = document.getElementById("emotionalIntelReflection").value;
    let emotionalIntelRelationshipsVal = document.getElementById("emotionalIntelRelationships").value;
    let emotionalIntelResponsibilityVal = document.getElementById("emotionalIntelResponsibility").value;
    let emotionalIntelTeamWorkVal = document.getElementById("emotionalIntelTeamWork").value;
    res.push(emotionalIntelAccountabilityVal);
    res.push(emotionalIntelBehaviorVal);
    res.push(emotionalIntelIndependenceVal);
    res.push(emotionalIntelReflectionVal);
    res.push(emotionalIntelRelationshipsVal);
    res.push(emotionalIntelResponsibilityVal);
    res.push(emotionalIntelTeamWorkVal);
    alert(res);
  }
</script>
