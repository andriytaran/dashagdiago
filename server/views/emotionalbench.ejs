<% layout('layout_main') -%>
<div id="content" class="content">
  <!-- begin breadcrumb -->
  <ol class="breadcrumb pull-right">
    <li class="breadcrumb-item"><a href="javascript:;">Home</a></li>
    <li class="breadcrumb-item active">Dashboard</li>
  </ol>
  <!-- end breadcrumb -->
  <!-- begin page-header -->
  <h1 class="page-header">
    <%=team %> Benchmark Input Page</h1>
  <!-- end page-header -->
  <!-- begin row -->
  <div class="row">
    <!-- begin col-6 -->
    <div class="col-lg-12">
      <!-- begin panel -->
      <div class="panel panel-inverse" id="basicInfo" data-sortable-id="index-7">
        <div class="panel-heading">
          <h4 class="panel-title">Academic attributes benchmark input</h4>
        </div>
        <div class="panel-body">
          <form id="emotionalform">
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="emotionalIntelAccountability">Emotional Intel Accountability Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Emotional Intel Accountability" id="emotionalIntelAccountability" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="emotionalIntelBehavior">Emotional Intel Behavior Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Emotional Intel Behavior" id="emotionalIntelBehavior" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="emotionalIntelIndependence">Emotional Intel Independence Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Emotional Intel Independence" id="emotionalIntelIndependence" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="emotionalIntelReflection">Emotional Intel Reflection Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Emotional Intel Reflection" id="emotionalIntelReflection" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="emotionalIntelRelationships">Emotional Intel Relationships Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Emotional Intel Relationships" id="emotionalIntelRelationships" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="emotionalIntelResponsibility">Emotional Intel Responsibility Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Emotional Intel Responsibility" id="emotionalIntelResponsibility" />
              </div>
            </div>
            <div class="form-group row m-b-15">
              <label class="col-form-label col-md-3" for="emotionalIntelTeamWork">Emotional Intel teamWork Benchmark</label>
              <div class="col-md-9">
                <input class="form-control m-b-5" type="text" placeholder="Emotional Intel teamWork" id="emotionalIntelTeamWork" />
              </div>
            </div>
            <div class="subbtn"><input type="submit" class="btn btn-lime" id="submit" value="Done"></div>
          </form>
