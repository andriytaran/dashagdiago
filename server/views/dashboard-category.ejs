<% layout('layout_main') -%>
<div id="content" class="content">
  <!-- begin breadcrumb -->
  <ol class="breadcrumb pull-right">
    <li class="breadcrumb-item"><a href="javascript:;">Home</a></li>
    <li class="breadcrumb-item active">Dashboard</li>
  </ol>
  <!-- end breadcrumb -->
  <!-- begin page-header -->
  <h1 class="page-header">
    <%=position %> category dashboard</h1>
  <!-- end page-header -->

  <!-- begin row -->
  <div class="row">
    <!-- begin col-3 -->
    <div class="col-lg-3 col-md-6">
      <div class="widget widget-stats bg-grey-darker">
        <div class="stats-icon"><i class="fa fa-users"></i></div>
        <div class="stats-info">
          <h4>Average score</h4>
          <p>
            <% if (overallScore) { %>
            <%= overallScore %>%
            <% } else { %>Not available
            <% } %>
          </p>
        </div>
        <div class="stats-link">
          <p> <br></p>
        </div>
      </div>
    </div>
    <!-- end col-3 -->
  </div>
  <!-- end row -->

  <!-- begin row -->
  <div class="row">
    <!-- begin col-8 -->
    <div class="col-lg-12">
      <!-- begin panel -->
      <div class="panel panel-inverse" data-sortable-id="index-1">
        <div class="panel-heading">
          <h4 class="panel-title">Cultural Fit</h4>
        </div>
        <div class="panel-body panel-height-5 culturalFdiv">
          <div class="height-xs">Top 10 players
            <div id="culturaltop">
              <div class="elLoader">
                <div class="loader"></div>
              </div>
            </div>
          </div>

          <div id="cultural-fit" class="height-sm">
            <div class="elLoader">
              <div class="loader"></div>
            </div>
          </div>
        </div>
      </div>
      <!-- end panel -->
    </div>
    <!-- end col-6 -->
  </div>
  <!-- end row -->

  <!-- start row -->
  <div class="row">
    <!-- begin col-lg-12 -->
    <div class="col-lg-12" id="overallBlock1">
      <div class="elLoader">
        <div class="loader"></div>
      </div>
    </div>
    <!-- end col-lg-12 -->
  </div>
</div>

<!-- start modal -->
<div class="modal fade" id="chartmodal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalplayerstitle">Players</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div id="players"></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
<!-- end modal -->

<!-- Modal -->
<div id="loginModal" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="container">

      <div class="row" id="pwd-container">
        <div class="col-md-4"></div>

        <div class="col-md-4">
          <section class="login-form">
            <form method="post" action="#" role="login">
              <img src="/assets/img/logo.png" class="img-responsive" alt="" style="width: 100px" />
              <input type="email" name="email" placeholder="Email" required class="form-control input-lg" value="enteryourmail@gmail.com" />

              <input type="password" class="form-control input-lg" id="password" placeholder="Password" required="" />


              <div class="pwstrength_viewport_progress"></div>


              <button type="submit" name="go" class="btn btn-lg btn-primary btn-block">Sign in</button>
              <div>
                <a href="#">Create account</a> or <a href="#">reset password</a>
              </div>

            </form>

            <div class="form-links">
              <a href="#">www.website.com</a>
            </div>
          </section>
        </div>

        <div class="col-md-4"></div>


      </div>
    </div>

  </div>
</div>
