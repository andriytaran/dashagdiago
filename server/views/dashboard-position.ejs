<% layout('layout_main') -%>
<div id="content" class="content">
  <!-- begin breadcrumb -->
  <ol class="breadcrumb pull-right">
    <li class="breadcrumb-item"><a href="javascript:;">Home</a></li>
    <li class="breadcrumb-item active">Dashboard</li>
  </ol>
  <!-- end breadcrumb -->

  <!-- begin page-header -->
  <h1 class="page-header">
    <%=position %> position dashboard</h1>
    <hr>
    <strong><i>Note this data represents public  <%=position %>  players from schools accross the USA based on your registered Athletic and Academic benchmarks</i></strong>
  <!-- end page-header -->

  <!-- begin row -->
  <div class="row">

    <!-- begin col-8 -->
    <div class="col-lg-12">

      <!-- begin panel -->
      <div class="panel panel-inverse" data-sortable-id="index-1">
        <div class="panel-heading">
          <h4 class="panel-title">Cultural Fit | Top <%=position %> potential recruits to review</h4>
        </div>
        <div class="panel-body panel-height-5 culturalFdiv">
          <div class="height-xs">Top 10 <%=position %> players
            <div id="culturaltop">
              <div class="elLoader">
                <div class="loader"></div>
              </div>
            </div>
          </div>

          <div id="cultural-fit" class="height-sm">
            <div class="elLoader">
              <div class="loader"></div>
            </div>
          </div>
        </div>

      </div>
      <!-- end panel -->

    </div>
    <!-- end col-6 -->

  </div>
  <!-- end row -->

  <!-- start row -->
  <div class="row">

    <!-- begin col-lg-12 -->
    <div class="col-lg-12" id="overallBlock1">

      <div class="elLoader">
        <div class="loader"></div>
      </div>

    </div>
    <!-- end col-lg-12 -->

  </div>
  <!-- end row -->
</div>

<!-- start modal -->
<div class="modal fade" id="chartmodal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
  aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalplayerstitle">Players</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div id="players"></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
<!-- end modal -->

